{
   "name": "SuperCharged-Angular2-Webpack2-DotNET-Starter",
   "version": "2.0.0",
   "description": "A complete starter kit for building Single Page Apps featuring Angular 2, Typescript, Webpack, Authentication, NPM Scripts, Unit Tests, and E2E Tests. Includes optional .NET Core 1.0 integration for working with a C# backend and Continuous Integration scripts for using source control on a TFS build server.",
   "author": "Nathan P. Forked from 'Anuglar2-webpack-starter' on github",
   "homepage": "https://github.com/angularclass/angular2-webpack-typescript-dotnet-starter",
   "license": "MIT",
   "scripts": {
      "devserver:aot": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.development.js --progress --profile --inline --hot --compress",
      "devserver:jit": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.development.js --progress --profile --inline --hot --compress",
      "compile:build-dev:aot": "npm run compile:aot && npm run build:dev:aot",
      "compile:build-production:aot": "npm run clean:compile && npm run compile:aot && npm run build:production:aot",
      "compile:aot": "./node_modules/.bin/ngc -p ./tsconfig.aot.json",
      "build:dev:aot": "npm run clean:wwwroot && cross-env NODE_ENV=development webpack --hot --profile --progress --colors --display-cached --watch",
      "build:dev:jit": "npm run clean:wwwroot && cross-env NODE_ENV=development webpack --hot --profile --progress --colors --display-cached --watch",
      "build:production:aot": "npm run clean:wwwroot && cross-env NODE_ENV=production webpack --progress --profile --colors --display-cached --bail --watch",
      "build:production:jit": "npm run clean:wwwroot && cross-env NODE_ENV=production webpack --progress --profile --colors --display-cached --bail --watch",
      "build:dlls": "npm run clean:wwwroot:dlls && cross-env NODE_ENV=dlls webpack --progress --profile --colors --display-cached --bail",
      "update:views": "npm run copy:index.html && npm run rename:index.html",
      "precompile:aot": "npm run clean:compile",
      "npm:install": "npm install --no-optional",
      "clean:compile:misc": "cd ./src/app-components && del *.ngfactory.ts *.ngfactory.js *.js.map *.metadata.json /s",
      "clean:compile": "rimraf -- aot-compiled wwwroot/compiled-src",
      "clean:install": "npm run clean && npm run install:all",
      "dirty:install": "npm run install:all",
      "install:all": "npm set progress=false && npm install --no-optional --depth 0",
      "postinstall:all": "npm run compile:aot && npm run build:dev:jit",
      "copy:index.html": "copyfiles --up 1 wwwroot/index.html Views/Shared",
      "prerename:index.html": "rimraf Views/Shared/_Layout.cshtml",
      "rename:index.html": "cd ./tools && elevate.exe  -c renamer --find \"index.html\" --replace \"_Layout.cshtml\" \"../Views/Shared/*\"",
      "build:stats": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
      "ci": "npm run lint && npm test:karma && npm run e2e",
      "clean": "npm run rimrafglobal -- node_modules doc coverage typings wwwroot/* bower_components ../../cache",
      "clean:happypack": "npm run rimraf -- ../../cache/happypack",
      "preclean:install:globals": "npm run install:globals:all",
      "clean:install:globals": "npm run clean",
      "clean:wwwroot": "rimraf -- wwwroot/js/* wwwroot/assets/* wwwroot/css/*",
      "clean:wwwroot:dlls": "rimraf -- wwwroot/dlls/*",
      "cleancache": "npm cache clean",
      "devserver": "npm run devserver:aot",
      "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
      "e2e": "npm run protractor",
      "e2e:live": "npm run e2e -- --elementExplorer",
      "e2e:server": "npm run start && npm run protractor",
      "eslint": "eslint",
      "install:globals:all": "npm install typescript webpack-dev-server rimraf webpack renamer -g --no-optional",
      "install:globals:rimraf": "npm install rimraf -g --no-optional",
      "launch:Debug": "npm version",
      "launch:FrontEnd": "npm run build:dev:jit",
      "launch:Release": "npm run tfsbuild",
      "launch:Tests-E2E": "start npm run e2e",
      "launch:Tests-Unit": "start npm run test:karma",
      "lint": "npm run tslint \"src/**/*.ts\" && npm run eslint \"config/**/*.js\"",
      "onetimesetup": "npm run install:globals:all && npm run install:all && npm run devserver:jit",
      "postversion": "git push && git push --tags",
      "pree2e": "npm run webdriver:update -- --standalone",
      "pree2e:server": "npm run clean:wwwroot",
      "prelint": "tsc -m commonjs --noImplicitAny config/rules/importDestructuringSpacingRule.ts node_modules/tslint/lib/tslint.d.ts",
      "preonetimesetup": "npm run remove-web-tools",
      "preversion": "npm test:karma",
      "protractor": "protractor",
      "remove-web-tools": "cd ../PowerShell && elevate.exe -c renamer --find \"External\" --replace \"External_DISABLED\" \"%ProgramFiles(x86)%/Microsoft Visual Studio 14.0/Common7/IDE/Extensions/Microsoft/Web Tools/External",
      "postremove-web-tools": "cd ../PowerShell && elevate.exe -c renamer --find \"External\" --replace \"External_DISABLED\" \"%ProgramFiles(x86)%/Microsoft Visual Studio 14.0/Web/External",
      "rimraf": "rimraf",
      "rimrafglobal": "npm uninstall rimraf && rimraf",
      "run-powershellscript-example": "node ../PowerShell/RunPowerShellScript.js",
      "server:all": "start npm run devserver:aot & npm run apiserver",
      "devserver:jit:dashboard": "start webpack-dashboard -- cross-env NODE_ENV=development webpack-dev-server --config config/webpack.development.js --profile --progress",
      "start": "npm run devserver:jit",
      "test": "npm run test:all",
      "test:all": "npm test:karma && npm run e2e",
      "test:e2e": "npm run e2e",
      "test:karma": "karma start",
      "pretfsinstall": "npm run install:globals:all",
      "tfsinstall": "npm run clean:install",
      "tfsbuild": "npm run build:production:jit",
      "tfsinstall:cleangenerated": "npm run clean:happypack && npm run clean:wwwroot",
      "tslint": "tslint",
      "typedoc": "typedoc",
      "version": "npm run build",
      "watch": "start npm run watch:dev",
      "watch:dev": "start npm run build:dev:jit -- --watch",
      "watch:dev:hmr": "start npm run watch:dev -- --hot",
      "watch:Release": "start npm run build:production:jit -- --watch",
      "watch:test": "start npm run test:karma -- --auto-watch --no-single-run",
      "webdriver:start": "npm run webdriver-manager start",
      "webdriver:update": "npm run webdriver-manager update",
      "webdriver-manager": "webdriver-manager",
      "webpack": "webpack",
      "webpack-dev-server": "webpack-dev-server",
      "clean:dotnetfiles": "rimraf bin obj",
      "server:dotnetcore:dev": "set \"ASPNETCORE_ENVIRONMENT=Webpack\" && dotnet run",
      "server:dotnetcore:production": "set \"ASPNETCORE_ENVIRONMENT=Production\" && dotnet run"
   },
