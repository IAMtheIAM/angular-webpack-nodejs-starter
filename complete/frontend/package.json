{
   "name": "Angular2-Webpack2-DotNET-Starter",
   "version": "2.2.0",
   "description": "A complete starter kit for building Single Page Apps featuring Angular 2, Typescript, Webpack, Authentication, NPM Scripts, Unit Tests, and E2E Tests. Includes .NET Core 1.0 integration for working with a C# backend and Continuous Integration scripts for using source control on a TFS build server.",
   "author": "IAMtheIAM",
   "homepage": "https://github.com/IAMtheIAM/angular2-webpack2-dotnet-starter",
   "license": "MIT",
  "scripts": {
    "devserver:aot": "npm run check:dlls && cross-env NODE_ENV=development webpack-dev-server --config config/webpack.development.js --profile --progress --inline --hot --compress --content-base wwwroot",
    "devserver:jit": "npm run check:dlls && cross-env NODE_ENV=development webpack-dev-server --config config/webpack.development.js  --profile --progress --inline --hot --compress --content-base wwwroot",
    "check:dlls": "path-exists \"wwwroot/dlls\" && echo \"DLLS folder exists, continuing...\" || echo \"DLLS folder doesn't exist, building now...\" && npm run build:dlls",

    "compile:build-dev:aot": "npm run clean:compile && npm run compile:aot && npm run build:dev:aot",
    "compile:build-production:aot": "npm run clean:compile && npm run compile:aot && npm run build:production:aot",
    "compile:build-production:jit:watch": "npm run clean:compile && npm run compile:aot && npm run build:production:jit:watch",
    "compile:aot": "./node_modules/.bin/ngc -p ./tsconfig.aot.json",

    "build:dev": "npm run check:dlls && npm run clean:wwwroot && cross-env NODE_ENV=development webpack --hot --profile --progress --colors --display-cached && npm run copy:wwwroot",
    "build:dev:aot": "npm run build:dev",
    "build:dev:jit": "npm run build:dev",
    "build:dev:jit:watch": "npm run build:dev:jit -- --watch",

    "build:production": "npm run check:dlls && npm run clean:wwwroot && cross-env NODE_ENV=production webpack --progress --profile --colors --display-cached --bail && npm run copy:wwwroot",
    "build:production:aot": "npm run build:production",
    "build:production:jit": "npm run build:production",
    "build:production:jit:watch": "npm run build:production:jit -- --watch",

    "build:dlls": "npm run clean:wwwroot:dlls && cross-env NODE_ENV=dlls webpack --progress --profile --colors --display-cached --bail",
    "build:stats": "cross-env NODE_ENV=production webpack --profile --json > stats.json",

    "precompile:aot": "npm run clean:compile",
    "npm:install": "npm install --no-optional",

    "clean": "npm run rimrafglobal -- node_modules doc coverage typings wwwroot/* bower_components ../../cache dotnetcore/wwwroot dotnetframework/wwwroot",
    "cleancache": "npm cache clean",
    "clean:happypack": "npm run rimraf -- ../../cache/happypack",
    "preclean:install:globals": "npm run install:globals:all",
    "clean:install:globals": "npm run clean",
    "clean:wwwroot": "rimraf -- wwwroot/js wwwroot/assets wwwroot/css ../dotnetcore/wwwroot ../dotnetframework/Frontend.Webhost/wwwroot",
    "clean:wwwroot:dlls": "rimraf -- wwwroot/dlls ../dotnetcore/wwwroot/dlls ../dotnetframework/Frontend.Webhost/wwwroot/dlls",
    "clean:compile:misc": "cd ./src && del *.ngfactory.ts *.ngfactory.js *.js.map *.metadata.json /s",
    "clean:compile": "rimraf -- compiled",
    "clean:install": "npm run clean && npm run install:all:build",
    "dirty:install": "npm run install:all:build",

    "install:all": "npm set progress=false && npm install --no-optional --depth 0",
    "install:all:build": "npm set progress=false && npm install --no-optional --depth 0 && npm run compile:build-production:aot",

    "update:views": "npm run copy:index.html && npm run rename:index.html",
    "prerename:index.html": "rimraf Views/Shared/_Layout.cshtml",
    "rename:index.html": "cd ./tools && elevate.exe  -c renamer --find \"index.html\" --replace \"_Layout.cshtml\" \"../Views/Shared/*\"",

    "copy:index.html": "ncp wwwroot/index.html Views/Shared",
    "copy:wwwroot": "npm run copy:wwwroot:dotnetcore && npm run copy:wwwroot:dotnetframework",
    "copy:wwwroot:dotnetcore": "ncp wwwroot \"../dotnetcore/wwwroot\"",
    "copy:wwwroot:dotnetframework": "ncp wwwroot \"../dotnetframework/Frontend.Webhost/wwwroot\"",

    "ci": "npm run lint && npm test:karma && npm run e2e",
    "devserver": "npm run devserver:aot",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e:server": "npm run start && npm run protractor",
    "eslint": "eslint",
    "install:globals:all": "npm install typescript webpack-dev-server rimraf webpack renamer path-exists -g --no-optional",
    "install:globals:rimraf": "npm install rimraf -g --no-optional",
    "launch:Debug": "npm version",
    "launch:Release": "npm run tfsbuild",
    "launch:~JIT.Dev-Webpack.Dev.Server": "node spawner.js run \"npm run devserver:jit\"",
    "launch:~JIT.Prod-Compile+Build.Prod.Watch.Changes": "node spawner.js runexternal \"npm run compile:build-production:jit:watch\"",
    "launch:~AOT.Prod-Compile+Build.Prod": "npm run compile:build-production:aot",
    "launch:Tests-E2E": "start npm run e2e",
    "launch:Tests-Unit": "start npm run test:karma",
    "lint": "npm run tslint \"src/**/*.ts\" && npm run eslint \"config/**/*.js\"",
    "onetimesetup": "npm run dotnetcore:restore && npm run install:globals:all && npm run install:all:build",
    "postversion": "git push && git push --tags",
    "pree2e": "npm run webdriver:update -- --standalone",
    "pree2e:server": "npm run clean:wwwroot",
    "prelint": "tsc -m commonjs --noImplicitAny config/rules/importDestructuringSpacingRule.ts node_modules/tslint/lib/tslint.d.ts",
    "preonetimesetup": "npm run remove-web-tools",
    "preversion": "npm test:karma",
    "protractor": "protractor",
    "remove-web-tools": "cd ./tools && elevate.exe -c renamer --find \"External\" --replace \"External_DISABLED\" \"%ProgramFiles(x86)%/Microsoft Visual Studio 14.0/Common7/IDE/Extensions/Microsoft/Web Tools/External",
    "postremove-web-tools": "cd ./tools && elevate.exe -c renamer --find \"External\" --replace \"External_DISABLED\" \"%ProgramFiles(x86)%/Microsoft Visual Studio 14.0/Web/External",
    "rimraf": "rimraf",
    "rimrafglobal": "npm uninstall rimraf && rimraf",
    "run-powershellscript-example": "node ../PowerShell/RunPowerShellScript.js",
    "server:all": "start npm run devserver:aot & npm run apiserver",
    "devserver:jit:dashboard": "start webpack-dashboard -- cross-env NODE_ENV=development webpack-dev-server --config config/webpack.development.js --profile --progress",
    "start": "npm run devserver:jit",
    "test": "npm run test:all",
    "test:all": "npm test:karma && npm run e2e",
    "test:e2e": "npm run e2e",
    "test:karma": "karma start",
    "pretfsinstall": "npm run install:globals:all",
    "tfsinstall": "npm run clean && npm run npm:install",
    "tfsbuild": "npm run compile:build-production:aot",
    "tfsinstall:cleangenerated": "npm run clean:happypack && npm run clean:wwwroot",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch": "start npm run watch:dev",
    "watch:dev": "start npm run build:dev:jit -- --watch",
    "watch:dev:hmr": "start npm run watch:dev -- --hot",
    "watch:Release": "start npm run build:production:aot -- --watch",
    "watch:test": "start npm run test:karma -- --auto-watch --no-single-run",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver-manager": "webdriver-manager",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "dotnetcore:server:dev": "cd ../dotnetcore && set \"ASPNETCORE_ENVIRONMENT=Webpack\" && dotnet run",
    "dotnetcore:server:production": "cd ../dotnetcore && set \"ASPNETCORE_ENVIRONMENT=Production\" && dotnet run",
    "dotnetcore:clean": "cd ../dotnetcore && rimraf bin obj",
    "dotnetcore:restore": "cd ../dotnetcore && dotnet restore",
    "precommit-msg": "echo 'Pre-commit checks... Currently disabled. Consider running \"build:dev:jit\" and \"compile:build-production:aot\" before committing to ensure the build succeeds.' && exit 0"
  },
   "pre-commit": [
      "precommit-msg"
   ],
